https://leetcode.com/problems/subtract-the-product-and-sum-of-digits-of-an-integer/
//write a program to subtract-the-product-and-sum-of-digits-of-an-integer.
class Solution {
public int subtractProductAndSum(int n) {
int r,s;
int sum=0,product=1;
int res;
while(n!=0)
{
r=n%10;
sum+=r;
product*=r;
n=n/10;
}
res=-(sum-product);
return(res);
}
}
