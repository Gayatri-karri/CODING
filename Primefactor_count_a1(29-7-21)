import java.util.*;
class Primefactor_count
{
public static void main(String args[])
{int count=0;
Scanner sc=new Scanner(System.in);
int n=sc.nextInt();
while(n%2==0)
{
count++;
n=n/2;
}
for(int i=3;i<=Math.sqrt(n);i+=2)
{
while(n%i==0)
{
count++;
n=n/i;
}
}
if(n>2)
count++;
System.out.print(count);
}
}
