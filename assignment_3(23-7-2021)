import java.util.*;
class assign3
{
public static void main(String args[])
{
Scanner sc=new Scanner(System.in);
System.out.println("Enter size n");
int n=sc.nextInt();
Stack<Integer>st=new Stack<>();
for(int i=0;i<n;i++)
{
st.push(sc.nextInt());
}
System.out.println("MaximumValue:"+Collections.max(st));
System.out.println("ManimumValue:"+Collections.min(st));
}
}
